@using CollegeInfo.Core
@model College
@{
    ViewData["Title"] = "List of All college";
}

 

    <table>
        <tbody id="body">
           
      
            <tr>
                <td><h3>@Model.CollegeName</h3></td>
                <td>Department:@Model.Department</td>
                <td>City: @Model.City</td>
                <td>
                    <a class="btn btn-lg"
                       asp-page="./Detail"
                       asp-route-restaurantId="@Model.Id">
                        </a>
                </td>
                <td>
                    <a class="btn btn-lg"
                       asp-page="./Edit"
                       asp-route-restaurantId="@Model.Id">
                    </a>
                </td>
                <td>
                    <a class="btn btn-lg"
                       asp-page="./Delete"
                       asp-route-restaurantId="@Model.Id">
                    </a>
                </td>
            </tr>
  
    </tbody>
    </table>